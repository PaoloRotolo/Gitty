<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.paolorotolo.gitty_reporter_example">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">
        <activity
            android:name="com.github.paolorotolo.gitty_reporter_example.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.github.paolorotolo.gitty_reporter_example.GittyReporterExample"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.github.paolorotolo.gitty_reporter_example.GittyReporterNoGuestExample"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.github.paolorotolo.gitty_reporter_example.GittyReporterNoLoginExample"
            android:label="@string/app_name" >
        </activity>
    </application>
</manifest>